{
  "manifest_version": 3,
  "name": "ResponsiBot",
  "version": "1.0",
  "description": "Analyze webpage accessibility, SEO, and performance",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
